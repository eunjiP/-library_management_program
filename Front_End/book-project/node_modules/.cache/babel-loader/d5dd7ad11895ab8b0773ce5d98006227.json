{"ast":null,"code":"import axios from 'axios';\nimport AlertModal from '../components/common/AlertModal.vue';\nexport default {\n  computed: {\n    AlertModal\n  },\n\n  data() {\n    return {\n      userid: '',\n      userpw: '',\n      modalShow: false,\n      msg: \"\"\n    };\n  },\n\n  methods: {\n    userLogin() {\n      if (this.userid.trim() !== '' && this.userpw.trim() !== '') {\n        const param = {\n          userid: this.userid,\n          userpw: this.userpw\n        };\n        axios.post('/user/signin', param).then(res => {\n          console.log(res);\n\n          if (res.status === 200 && isset(res.data.result)) {\n            this.msg = res.result;\n            this.modalShow = true;\n          }\n        });\n      }\n    },\n\n    changeShow() {\n      this.modalShow = false, this.msg = '';\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiBI,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,UAAP,MAAuB,qCAAvB;AAEJ,eAAe;EACXC,QAAQ,EAAE;IACND;EADM,CADC;;EAIXE,IAAI,GAAG;IACH,OAAO;MACHC,MAAM,EAAC,EADJ;MAEHC,MAAM,EAAC,EAFJ;MAGHC,SAAS,EAAE,KAHR;MAIHC,GAAG,EAAC;IAJD,CAAP;EAMH,CAXU;;EAYXC,OAAO,EAAE;IACLC,SAAS,GAAG;MACR,IAAG,KAAKL,MAAL,CAAYM,IAAZ,OAAuB,EAAvB,IAA6B,KAAKL,MAAL,CAAYK,IAAZ,OAAuB,EAAvD,EAA2D;QACvD,MAAMC,KAAI,GAAI;UACVP,MAAM,EAAC,KAAKA,MADF;UAEVC,MAAM,EAAC,KAAKA;QAFF,CAAd;QAIAL,KAAK,CAACY,IAAN,CAAW,cAAX,EAA2BD,KAA3B,EACCE,IADD,CACMC,GAAE,IAAK;UACTC,OAAO,CAACC,GAAR,CAAYF,GAAZ;;UACA,IAAGA,GAAG,CAACG,MAAJ,KAAe,GAAf,IAAsBC,KAAK,CAACJ,GAAG,CAACX,IAAJ,CAASgB,MAAV,CAA9B,EAAiD;YAC7C,KAAKZ,GAAL,GAAWO,GAAG,CAACK,MAAf;YACA,KAAKb,SAAL,GAAiB,IAAjB;UACJ;QACH,CAPD;MAQJ;IACH,CAhBI;;IAiBLc,UAAU,GAAG;MACT,KAAKd,SAAL,GAAiB,KAAjB,EACA,KAAKC,GAAL,GAAW,EADX;IAEJ;;EApBK;AAZE,CAAf","names":["axios","AlertModal","computed","data","userid","userpw","modalShow","msg","methods","userLogin","trim","param","post","then","res","console","log","status","isset","result","changeShow"],"sourceRoot":"","sources":["D:\\vue.js\\book-project\\src\\views\\UserSignin.vue"],"sourcesContent":["<template>\r\n   <div id=\"main\">\r\n        <div class=\"test\"><i id=\"userIcon\" class=\"fa-regular fa-circle-user\"></i></div>\r\n        <div class=\"input\">\r\n            <div><input type=\"text\" v-model=\"userid\" placeholder=\"학번\"></div>\r\n            <div><input type=\"password\" v-model=\"userpw\" placeholder=\"비밀번호\"></div>\r\n            <div class=\"btn cursor\" @click=\"userLogin\">LOGIN</div>\r\n        </div>\r\n   </div>\r\n   <div id=\"footer\">\r\n        <div class=\"btn cursor\" id=\"joinbtn\">JOIN</div>\r\n   </div>\r\n   <!-- <AlertModal :show=\"modalShow\" header=\"오류창\" :body=\"msg\" @close=\"changeShow\"></AlertModal> -->\r\n</template>\r\n\r\n<!-- <script src=\"https://kit.fontawesome.com/57749be668.js\" crossorigin=\"anonymous\"></script> -->\r\n<script>\r\n    import axios from 'axios';\r\n    import AlertModal from '../components/common/AlertModal.vue';\r\n\r\nexport default {\r\n    computed: {\r\n        AlertModal\r\n    },\r\n    data() {\r\n        return {\r\n            userid:'',\r\n            userpw:'',\r\n            modalShow: false,\r\n            msg:\"\"\r\n        }\r\n    },\r\n    methods: {\r\n        userLogin() {\r\n            if(this.userid.trim() !== '' && this.userpw.trim() !== '') {\r\n                const param = {\r\n                    userid:this.userid,\r\n                    userpw:this.userpw\r\n                }\r\n                axios.post('/user/signin', param)\r\n                .then(res => {\r\n                    console.log(res);\r\n                    if(res.status === 200 && isset(res.data.result)) {\r\n                        this.msg = res.result;\r\n                        this.modalShow = true;\r\n                    }\r\n                })\r\n            }\r\n        },\r\n        changeShow() {\r\n            this.modalShow = false,\r\n            this.msg = ''\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#main { background-color: #7BB3BC;\r\n    text-align: center;\r\n    margin-left: 10vw;\r\n    border-radius: 20vw 0 0 20vw;\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr;\r\n}\r\n.input {display: grid;\r\n    align-content: space-evenly;\r\n    color: #FFFFFF;\r\n    }\r\ninput { width: 70%; height: 5vh; text-align: center; border: none;\r\n    border-radius: 5px; background-color: #B2EBF4;}\r\n.btn {font-size: 2rem; font-weight: bold;}\r\n#joinbtn {color: #7BB3BC;}\r\n#userIcon { font-size: 5rem;}\r\n#footer {text-align: center; margin-top: 4vh;}\r\n</style>"]},"metadata":{},"sourceType":"module"}