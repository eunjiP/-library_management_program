{"ast":null,"code":"import SignupHeader from './components/signup/SignupHeader.vue';\nimport SignupPut from './components/signup/SignupPut.vue';\nimport AlertModal from './components/common/AlertModal.vue';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      modalShow: false,\n      body: \"\"\n    };\n  },\n\n  components: {\n    SignupHeader,\n    SignupPut,\n    AlertModal\n  },\n  methods: {\n    userSign(id, pw, nm) {\n      axios.post('/user/signup', {\n        'id': id,\n        'pw': pw,\n        'nm': nm\n      }).then(res => {\n        if (res.status !== 200) {\n          this.modalShow = true;\n          this.body = \"회원가입이 실패하였습니다.\";\n        }\n      });\n    },\n\n    idnoneAlert(val) {\n      this.modalShow = val;\n      this.body = \"아이디를 입력해주세요\";\n    },\n\n    pwFalseAlert(val) {\n      this.modalShow = val;\n      this.body = \"입력하신 비밀번호가 다릅니다. 다시 입력해주세요.\";\n    },\n\n    changeShow() {\n      this.modalShow = false;\n      this.body = \"\";\n    }\n\n  }\n};","map":{"version":3,"mappings":"AASA,OAAOA,YAAP,MAAyB,sCAAzB;AACA,OAAOC,SAAP,MAAsB,mCAAtB;AACA,OAAOC,UAAP,MAAuB,oCAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,SAAQ,EAAI,KADP;MAELC,IAAG,EAAI;IAFF,CAAP;EAID,CANY;;EAObC,UAAU,EAAE;IACVP,YADU;IAEVC,SAFU;IAGVC;EAHU,CAPC;EAYbM,OAAO,EAAE;IACPC,QAAQ,CAACC,EAAD,EAAKC,EAAL,EAASC,EAAT,EAAa;MACnBT,KAAK,CAACU,IAAN,CAAW,cAAX,EAA2B;QACzB,MAAKH,EADoB;QAEzB,MAAKC,EAFoB;QAGzB,MAAKC;MAHoB,CAA3B,EAKCE,IALD,CAKMC,GAAE,IAAK;QACX,IAAGA,GAAG,CAACC,MAAJ,KAAe,GAAlB,EAAuB;UACrB,KAAKX,SAAL,GAAiB,IAAjB;UACA,KAAKC,IAAL,GAAY,gBAAZ;QACF;MACD,CAVD;IAWD,CAbM;;IAcPW,WAAW,CAACC,GAAD,EAAM;MACf,KAAKb,SAAL,GAAiBa,GAAjB;MACA,KAAKZ,IAAL,GAAY,aAAZ;IACD,CAjBM;;IAkBPa,YAAY,CAACD,GAAD,EAAM;MAChB,KAAKb,SAAL,GAAiBa,GAAjB;MACA,KAAKZ,IAAL,GAAY,6BAAZ;IACD,CArBM;;IAsBPc,UAAU,GAAG;MACX,KAAKf,SAAL,GAAiB,KAAjB;MACA,KAAKC,IAAL,GAAY,EAAZ;IACF;;EAzBO;AAZI,CAAf","names":["SignupHeader","SignupPut","AlertModal","axios","data","modalShow","bod