{"ast":null,"code":"import axios from 'axios';\nimport AlertModal from '../components/common/AlertModal.vue';\nexport default {\n  components: {\n    AlertModal\n  },\n\n  data() {\n    return {\n      userid: '',\n      userpw: '',\n      modalShow: false,\n      msg: \"\"\n    };\n  },\n\n  methods: {\n    userLogin() {\n      if (this.userid.trim() !== '' && this.userpw.trim() !== '') {\n        const param = {\n          userid: this.userid,\n          userpw: this.userpw\n        };\n        axios.post('/user/signin', param).then(res => {\n          if (res.status === 200 && res.data.result) {\n            this.msg = res.data.result;\n            this.modalShow = true;\n          }\n        });\n      }\n    },\n\n    changeShow() {\n      this.modalShow = false, this.msg = '';\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiBI,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,UAAP,MAAuB,qCAAvB;AAEJ,eAAe;EACXC,UAAU,EAAE;IACRD;EADQ,CADD;;EAIXE,IAAI,GAAG;IACH,OAAO;MACHC,MAAM,EAAC,EADJ;MAEHC,MAAM,EAAC,EAFJ;MAGHC,SAAS,EAAE,KAHR;MAIHC,GAAG,EAAC;IAJD,CAAP;EAMH,CAXU;;EAYXC,OAAO,EAAE;IACLC,SAAS,GAAG;MACR,IAAG,KAAKL,MAAL,CAAYM,IAAZ,OAAuB,EAAvB,IAA6B,KAAKL,MAAL,CAAYK,IAAZ,OAAuB,EAAvD,EAA2D;QACvD,MAAMC,KAAI,GAAI;UACVP,MAAM,EAAC,KAAKA,MADF;UAEVC,MAAM,EAAC,KAAKA;QAFF,CAAd;QAIAL,KAAK,CAACY,IAAN,CAAW,cAAX,EAA2BD,KAA3B,EACCE,IADD,CACMC,GAAE,IAAK;UACT,IAAGA,GAAG,CAACC,MAAJ,KAAe,GAAf,IAAsBD,GAAG,CAACX,IAAJ,CAASa,MAAlC,EAA0C;YACtC,KAAKT,GAAL,GAAWO,GAAG,CAACX,IAAJ,CAASa,MAApB;YACA,KAAKV,SAAL,GAAiB,IAAjB;UACJ;QACH,CAND;MAOJ;IACH,CAfI;;IAgBLW,UAAU,GAAG;MACT,KAAKX,SAAL,GAAiB,KAAjB,EACA,KAAKC,GAAL,GAAW,EADX;IAEJ;;EAnBK;AAZE,CAAf","names":["axios","AlertModal","components","data","userid","userpw","modalShow","msg","methods","userLogin","trim","param","post","then","res","status","result","changeShow"],"sourceRoot":"","sources":["D:\\vue.js\\book-project\\src\\views\\UserSignin.vue"],"sourcesContent":["<template>\r\n   <div id=\"main\">\r\n        <div class=\"test\"><i id=\"userIcon\" class=\"fa